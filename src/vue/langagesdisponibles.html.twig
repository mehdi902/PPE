{% extends "base.html.twig" %}
{% block title %} {{ parent() }} Validation email {% endblock %}

{% block contenu %} 

       
    <div class="card-body mx-auto" style="max-width: 90%" >

        <div class="card card-header">
            <h1><span class="badge "> Sur notre sites nos développeurs maitrises les langages listés ci-dessous</span> </h1>
        </div>
        
        
            <div class="panel panel-default mx-auto" > 

            <div class="table-responsive border mx-auto" style="width: 100%"> 
                <table class="table table-hover mx-auto" style="width: 100%; height: 100%">
                    


                    {% for langage in listepublique %}
                        <tr>
                            <td>
                                <h4>
                                    <strong>{{ langage.libelle }} </strong>
                                </h4>
                            </td>

                        </tr> 

                    {% endfor %}    
                </table>
            </div>
            </div>
           
    </div>
               
{% endblock %}