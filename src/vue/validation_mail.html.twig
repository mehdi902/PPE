{% extends "base.html.twig" %}
{% block title %} {{ parent() }} Validation email {% endblock %}

{% block contenu %} 
    
    <h1>Valider votre email</h1>
    
    <form>
  <div class="form-group">
    <label for="InputEmail1">Email</label>
    <input type="email" class="form-control" id="InputEmail1" name="InputEmail1" aria-describedby="emailHelp" placeholder="Email">
    
  </div>
  <div class="form-group">
    <label for="InputPassword1">Mot de passe</label>
    <input type="password" class="form-control" id="InputPassword1" name="InputPassword1" placeholder="Mot de passe">
  </div>
  <button type="submit" class="btn btn-primary" name="btValidation" id="btValidation">Valider</button>
</form>

{% endblock %} 